<style>
  :root {
    --navy: #0d1117;
    --surface: #161b22;
    --border: #30363d;
    --accent: #e94560;
    --gold: #f5c518;
    --light: #e6edf3;
    --muted: #8b949e;
    --green: #3fb950;
    --blue: #58a6ff;
    --purple: #bc8cff;
    --cyan: #39d4e0;
    --terminal-bg: #0d1117;
    --terminal-border: #30363d;
  }

  body {
    margin: 0;
    font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif;
    background: var(--navy);
    color: var(--light);
    line-height: 1.6;
  }

  /* ─── Nav ─── */
  .site-nav {
    display: flex;
    align-items: center;
    gap: 8px;
    padding: 14px 24px;
    background: var(--surface);
    border-bottom: 1px solid var(--border);
    position: sticky;
    top: 0;
    z-index: 100;
  }
  .site-nav a {
    color: var(--muted);
    text-decoration: none;
    font-size: 0.9rem;
    font-weight: 500;
    padding: 6px 14px;
    border-radius: 6px;
    transition: color 0.2s, background 0.2s;
  }
  .site-nav a:hover { color: #fff; background: rgba(255,255,255,0.06); }
  .site-nav a.active { color: var(--gold); background: rgba(245,197,24,0.08); }
  .nav-spacer { flex: 1; }
  .version-badge {
    font-size: 0.7rem;
    color: var(--muted);
    font-family: "SF Mono", Monaco, "Cascadia Code", monospace;
    background: rgba(255,255,255,0.04);
    padding: 3px 8px;
    border-radius: 4px;
  }

  /* ─── Hero ─── */
  .hero {
    text-align: center;
    padding: 80px 20px 60px;
    background: linear-gradient(180deg, var(--surface) 0%, var(--navy) 100%);
    border-bottom: 1px solid var(--border);
  }
  .hero-label {
    display: inline-block;
    font-size: 0.75rem;
    text-transform: uppercase;
    letter-spacing: 2px;
    color: var(--gold);
    background: rgba(245,197,24,0.1);
    padding: 4px 16px;
    border-radius: 20px;
    margin-bottom: 20px;
    font-weight: 600;
  }
  .hero h1 {
    font-size: 3rem;
    margin: 0 0 16px;
    color: #fff;
    font-weight: 700;
    letter-spacing: -0.5px;
  }
  .hero h1 .highlight { color: var(--accent); }
  .hero .subtitle {
    font-size: 1.2rem;
    color: var(--muted);
    max-width: 640px;
    margin: 0 auto 32px;
    line-height: 1.7;
  }
  .hero-actions {
    display: flex;
    gap: 14px;
    justify-content: center;
    flex-wrap: wrap;
  }
  .btn-primary {
    display: inline-block;
    background: var(--accent);
    color: #fff;
    padding: 12px 28px;
    border-radius: 8px;
    font-size: 1rem;
    font-weight: 600;
    text-decoration: none;
    transition: transform 0.15s, box-shadow 0.15s;
  }
  .btn-primary:hover {
    transform: translateY(-1px);
    box-shadow: 0 4px 16px rgba(233,69,96,0.3);
  }
  .btn-secondary {
    display: inline-block;
    background: transparent;
    color: var(--light);
    padding: 12px 28px;
    border-radius: 8px;
    font-size: 1rem;
    font-weight: 600;
    text-decoration: none;
    border: 1px solid var(--border);
    transition: border-color 0.2s, background 0.2s;
  }
  .btn-secondary:hover {
    border-color: var(--muted);
    background: rgba(255,255,255,0.03);
  }

  /* ─── Install banner ─── */
  .install-banner {
    background: var(--surface);
    border: 1px solid var(--border);
    border-radius: 10px;
    max-width: 560px;
    margin: -28px auto 0;
    padding: 16px 24px;
    display: flex;
    align-items: center;
    gap: 14px;
    position: relative;
    z-index: 10;
  }
  .install-banner code {
    flex: 1;
    font-family: "SF Mono", Monaco, "Cascadia Code", monospace;
    font-size: 0.9rem;
    color: var(--green);
    background: none;
  }
  .install-banner .prompt { color: var(--muted); }

  /* ─── Sections ─── */
  .section {
    max-width: 960px;
    margin: 0 auto;
    padding: 80px 24px;
  }
  .section-label {
    font-size: 0.7rem;
    text-transform: uppercase;
    letter-spacing: 2.5px;
    color: var(--accent);
    font-weight: 700;
    margin-bottom: 8px;
  }
  .section h2 {
    font-size: 2rem;
    color: #fff;
    margin: 0 0 12px;
    font-weight: 700;
  }
  .section .section-desc {
    color: var(--muted);
    font-size: 1.05rem;
    max-width: 600px;
    margin-bottom: 40px;
  }

  /* ─── Terminal block ─── */
  .terminal {
    background: var(--terminal-bg);
    border: 1px solid var(--terminal-border);
    border-radius: 10px;
    overflow: hidden;
    margin: 24px 0;
    font-family: "SF Mono", Monaco, "Cascadia Code", "Fira Code", monospace;
    font-size: 0.82rem;
    line-height: 1.65;
  }
  .terminal-header {
    background: var(--surface);
    padding: 8px 16px;
    display: flex;
    align-items: center;
    gap: 8px;
    border-bottom: 1px solid var(--terminal-border);
  }
  .terminal-dot {
    width: 10px;
    height: 10px;
    border-radius: 50%;
    display: inline-block;
  }
  .terminal-dot.red { background: #ff5f57; }
  .terminal-dot.yellow { background: #febc2e; }
  .terminal-dot.green { background: #28c840; }
  .terminal-title {
    flex: 1;
    text-align: center;
    font-size: 0.75rem;
    color: var(--muted);
  }
  .terminal-body {
    padding: 20px;
    overflow-x: auto;
    white-space: pre;
    color: var(--light);
  }
  .terminal-body .prompt { color: var(--green); }
  .terminal-body .cmd { color: #fff; font-weight: 600; }
  .terminal-body .comment { color: #6e7681; }
  .terminal-body .string { color: #a5d6ff; }
  .terminal-body .keyword { color: var(--purple); }
  .terminal-body .value { color: var(--cyan); }
  .terminal-body .accent { color: var(--accent); }
  .terminal-body .gold { color: var(--gold); }
  .terminal-body .dim { color: var(--muted); }

  /* ─── Agent cards ─── */
  .agent-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 16px;
    margin-top: 32px;
  }
  .agent-card {
    background: var(--surface);
    border: 1px solid var(--border);
    border-radius: 10px;
    padding: 20px;
    transition: border-color 0.2s, transform 0.15s;
  }
  .agent-card:hover {
    border-color: var(--muted);
    transform: translateY(-2px);
  }
  .agent-icon {
    font-size: 1.4rem;
    margin-bottom: 8px;
    display: block;
  }
  .agent-card h3 {
    font-size: 1rem;
    margin: 0 0 6px;
    color: #fff;
  }
  .agent-card p {
    font-size: 0.82rem;
    color: var(--muted);
    margin: 0;
    line-height: 1.5;
  }
  .agent-card .agent-scope {
    margin-top: 10px;
    font-size: 0.72rem;
    font-family: "SF Mono", Monaco, monospace;
    color: var(--blue);
    background: rgba(88,166,255,0.08);
    padding: 3px 8px;
    border-radius: 4px;
    display: inline-block;
  }

  /* ─── Feature cards ─── */
  .feature-row {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 24px;
    margin-top: 32px;
  }
  @media (max-width: 700px) {
    .feature-row { grid-template-columns: 1fr; }
  }
  .feature-card {
    background: var(--surface);
    border: 1px solid var(--border);
    border-radius: 10px;
    padding: 28px;
  }
  .feature-card h3 {
    font-size: 1.1rem;
    color: #fff;
    margin: 0 0 8px;
  }
  .feature-card p {
    font-size: 0.9rem;
    color: var(--muted);
    margin: 0;
    line-height: 1.6;
  }
  .feature-card .feature-tag {
    display: inline-block;
    font-size: 0.65rem;
    text-transform: uppercase;
    letter-spacing: 1.5px;
    padding: 2px 8px;
    border-radius: 4px;
    font-weight: 700;
    margin-bottom: 12px;
  }
  .feature-tag.cli { background: rgba(59,130,246,0.15); color: var(--blue); }
  .feature-tag.config { background: rgba(245,197,24,0.12); color: var(--gold); }
  .feature-tag.agents { background: rgba(188,140,255,0.12); color: var(--purple); }
  .feature-tag.workflow { background: rgba(63,185,80,0.12); color: var(--green); }

  /* ─── YAML highlight ─── */
  .yaml-key { color: var(--blue); }
  .yaml-val { color: #a5d6ff; }
  .yaml-comment { color: #6e7681; }
  .yaml-str { color: var(--green); }
  .yaml-list { color: var(--cyan); }

  /* ─── Workflow section ─── */
  .workflow-steps {
    display: flex;
    gap: 20px;
    margin-top: 32px;
    flex-wrap: wrap;
    justify-content: center;
  }
  .workflow-step {
    background: var(--surface);
    border: 1px solid var(--border);
    border-radius: 10px;
    padding: 24px 20px;
    text-align: center;
    flex: 1;
    min-width: 180px;
    max-width: 240px;
  }
  .step-number {
    width: 36px;
    height: 36px;
    border-radius: 50%;
    background: rgba(233,69,96,0.15);
    color: var(--accent);
    font-weight: 700;
    font-size: 1rem;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 12px;
  }
  .workflow-step h3 {
    font-size: 1rem;
    color: #fff;
    margin: 0 0 6px;
  }
  .workflow-step p {
    font-size: 0.82rem;
    color: var(--muted);
    margin: 0;
    line-height: 1.5;
  }

  /* ─── CTA ─── */
  .cta {
    text-align: center;
    padding: 80px 24px;
    border-top: 1px solid var(--border);
  }
  .cta h2 {
    font-size: 2rem;
    color: #fff;
    margin: 0 0 12px;
  }
  .cta p {
    color: var(--muted);
    font-size: 1.05rem;
    max-width: 500px;
    margin: 0 auto 28px;
  }

  /* ─── Footer ─── */
  .footer {
    text-align: center;
    padding: 24px;
    font-size: 0.8rem;
    color: #484f58;
    border-top: 1px solid var(--border);
  }
  .footer a { color: var(--muted); text-decoration: none; }
  .footer a:hover { color: var(--light); }
</style>

<nav class="site-nav">
  <a href="/" class="active">Home</a>
  <a href="/guide">Guide</a>
  <a href="/examples">Examples</a>
  <span class="nav-spacer"></span>
